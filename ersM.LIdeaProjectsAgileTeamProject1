[32m+[m[32mpackage projectvendingmachine;[m
[32m+[m
[32m+[m[32mimport java.util.Scanner;[m
[32m+[m
[32m+[m[32mpublic class Method {[m
[32m+[m
[32m+[m[32m    public double select(Products products) {[m
[32m+[m
[32m+[m[32m        System.out.println(products.toString());[m
[32m+[m
[32m+[m[32m        Scanner scan = new Scanner(System.in);[m
[32m+[m
[32m+[m[32m        do{[m
[32m+[m[32m            System.out.println("Lutfen almak istediginiz urunun numarasini giriniz (1'den 10'a kadar)");[m
[32m+[m[32m        products.setProductNum(scan.nextInt());[m
[32m+[m[32m        if (products.getProductNum()<1 || products.getProductNum()>10){[m
[32m+[m[32m            System.out.println("Gecerli bir product numarasi giriniz (1 ' den 10 ' a kadar)");[m
[32m+[m[32m        }[m
[32m+[m[32m        }while (products.getProductNum()<1 || products.getProductNum()>10);[m
[32m+[m[32m            return 0;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
